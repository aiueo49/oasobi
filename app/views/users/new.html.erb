<div>
  <h1 class="font-bold">Users#new</h1>
  <%= form_with model:@user do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.label :password_confirmation %>
      <%= f.password_field :password_confirmation %>

      <%= f.submit %>
     </div>
  <% end %>
</div>
